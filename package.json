  "description": "Useful functions for Twitch Extension Backend Services (EBS)",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "clearTestCache": "jest --clearCache",
    "clean": "rimraf dist/*",
    "dev": "tsc -w",
    "build": "npm run lint && npm run test && npm run clean && tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint --project tsconfig.json",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukemnet/twitch-ebs-tools.git"
  },
  "keywords": [
    "twitch",
    "extension",
    "tools",
    "ebs",
    "backend",
    "jwt"
  ],
  "author": {
    "name": "Łukasz Wójcik",
    "url": "https://www.lukem.net/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lukemnet/twitch-ebs-tools/issues"
  },
  "homepage": "https://github.com/lukemnet/twitch-ebs-tools#readme",
  "dependencies": {
    "jsonwebtoken": "8.5.1"
  },
  "devDependencies": {
    "@types/jest": "25.1.2",
    "@types/jsonwebtoken": "8.3.6",
    "jest": "24.9.0",
    "prettier": "1.19.1",
    "rimraf": "3.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "6.1.2",
    "tslint": "5.20.0",
    "tslint-config-airbnb": "5.11.2",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-immutable": "6.0.1",
    "tslint-plugin-prettier": "2.1.0",
    "typescript": "3.6.3"
  }
}
